{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "venv",
        ".venv"
    ],
    "ignore": [],
    "defineConstant": {},
    "stubPath": "",
    "reportMissingImports": true,
